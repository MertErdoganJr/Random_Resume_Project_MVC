@model MvcCvProject.Models.Entity.TblSkill

@{
    ViewBag.Title = "UpdateSkill";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Yetenek Düzenleme Sayfası</h4>
<br />
<form method="post">
    @Html.Label("Yetenek Adı")
    @Html.TextBoxFor(x => x.Skill, new { @class = "form-control" })
    <br />
    @Html.Label("Yetenek Beceri Oranı")
    @Html.TextBoxFor(x => x.Progress, new { @class = "form-control" })
    <br />
    <button class="btn btn-success">Güncelle</button>
</form>

