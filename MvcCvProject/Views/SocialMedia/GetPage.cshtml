@model MvcCvProject.Models.Entity.TblSocialMedia

@{
    ViewBag.Title = "GetPage";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Sosyal Medya Düzenleme Sayfası</h4>
<br />
<form method="post">
    @Html.Label("Sosyal Medya İsmi")
    @Html.TextBoxFor(x => x.SocialMediaName, new { @class = "form-control" })
    <br />
    @Html.Label("Link")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />
    @Html.Label("İkon")
    @Html.TextBoxFor(x => x.Icon, new { @class = "form-control" })
    <br />
    <button class="btn btn-success">Güncelle</button>
</form>

