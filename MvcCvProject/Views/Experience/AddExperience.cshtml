
@{
    ViewBag.Title = "AddExperience";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Yeni Deneyim Ekleme Sayfası</h4>
<br />
<form method="post">
    <input type="text" name="Title" class="form-control" placeholder="Başlğı Yazınız" />
    <br />
    <input type="text" name="SecondTitle" class="form-control" placeholder="Alt Başlığı Yazınız" />
    <br />
    <input type="text" name="Date" class="form-control" placeholder="Tarih" />
    <br />
    <textarea placeholder="Açıklama Giriniz" name="Description" class="form-control" rows="5"></textarea>
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>

